#include <stdio.h>

void main()
{
  int arr1[3][3],i,j;
  
printf("Input elements in the matrix :\n");

  for(i=0;i<3;i++)
  {
      for(j=0;j<3;j++)
      {
	      printf("element - [%d],[%d] : ",i,j);
	      scanf("%d",&arr1[i][j]);
      }
  }  
 
 
 printf("\nThe matrix is : \n");
  for(i=0;i<3;i++)
  {
      printf("\n");
      for(j=0;j<3;j++)
           printf("%d\t",arr1[i][j]);
  }
 printf("\n\n");
 
 int sum;

 sum=arr1[0][0]+arr1[1][1]+arr1[2][2];
printf("The Sum of all Diagonal Elements are: %d", sum);
 

}
